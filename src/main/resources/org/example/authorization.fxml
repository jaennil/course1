<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.AuthorizationController">
   <children>
      <TextField fx:id="usernameField" layoutX="226.0" layoutY="83.0" promptText="username" />
      <TextField fx:id="passwordField" layoutX="226.0" layoutY="127.0" promptText="password" />
      <Button fx:id="signInButton" layoutX="273.0" layoutY="175.0" mnemonicParsing="false" text="sign in" onAction="#onClickSignInButton"/>
      <Button fx:id="signUpButton" layoutX="270.0" layoutY="212.0" mnemonicParsing="false" text="sign up" onAction="#onClickSignUpButton"/>
   </children>
</AnchorPane>
